<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>



<AnchorPane prefHeight="800" prefWidth="1000"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.AdminController">
	<children>
		<Button fx:id="btnLogOut" layoutY="20" layoutX="800" mnemonicParsing="false" onAction="#logOut" prefHeight="39.0" prefWidth="139.0" text="Log Out" textFill="#572323">
         <font>
            <Font name="AR DESTINE" size="23.0" />
         </font>
     	 </Button>
		<Button fx:id="btnAddMember" onAction="#openAddMember" layoutX="408.0"
			layoutY="213.0" mnemonicParsing="false" prefHeight="79.0" prefWidth="185.0"
			text="Add Member" />
		<Button fx:id="btnEditMember" onAction="#EditMember" layoutX="413.0"
			layoutY="352.0" mnemonicParsing="false" prefHeight="79.0" prefWidth="175.0"
			text="Edit Member" />
		<Button fx:id="btnAddBook" onAction="#AddBook" layoutX="413.0"
			layoutY="482.0" mnemonicParsing="false" prefHeight="79.0" prefWidth="175.0"
			text="Add Book" />
	</children>
</AnchorPane>
